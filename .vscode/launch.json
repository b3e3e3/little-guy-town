{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDScript: Launch Current File",
            "type": "godot",
            "request": "launch",
            "scene": "current",
            "project": "",
            "additional_options": ""
        },
        // {
        //     "name": "Attach to Godot Game Process",
        //     "type": "cppvsdbg",
        //     "request": "attach",
        //     "processId": "${command:pickProcess}",
        //     "symbolSearchPath": "${workspaceFolder}/extensions/ventman/bin;C:/Godot/debug/bin"
        // },
        {
            "name": "Godot Editor",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "C:/Godot/debug/bin/godot.windows.editor.x86_64.console.exe",
            "args": [
                "${workspaceFolder}/project.godot",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/extensions/ventman",
            "environment": [],
            "console": "internalConsole",
            "symbolOptions": {
                // Update this path to where your PDB files for the extension are located.
                "searchPaths": [
                    "${workspaceFolder}/extensions/ventman/bin",
                    "C:/Godot/debug/bin",
                ],
                "loadAll": true,
            },
        },
    ]
}